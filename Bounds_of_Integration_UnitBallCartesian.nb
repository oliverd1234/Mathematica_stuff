            RowBox[{"xVal", "^", "2"}]}], "]"}], " ", 
          RowBox[{"Cos", "[", "t", "]"}]}], ",", 
         RowBox[{
          RowBox[{"Sqrt", "[", 
           RowBox[{
            RowBox[{"r", "^", "2"}], "-", 
            RowBox[{"xVal", "^", "2"}]}], "]"}], " ", 
          RowBox[{"Sin", "[", "t", "]"}]}]}], "}"}], ",", 
       RowBox[{"{", 
