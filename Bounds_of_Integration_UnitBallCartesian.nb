        RowBox[{"Directive", "[", 
         RowBox[{"Blue", ",", 
          RowBox[{"Opacity", "[", "0.3", "]"}]}], "]"}]}], ",", 
       RowBox[{"Mesh", "->", "None"}]}], "]"}]}], ";"}], 
   "\[IndentingNewLine]", "\n", 
   RowBox[{"(*", 
    RowBox[{"Choose", " ", "a", " ", "value", " ", "for", " ", "x"}], "*)"}], 
