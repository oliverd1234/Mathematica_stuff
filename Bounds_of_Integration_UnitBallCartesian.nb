Cell[BoxData[
 RowBox[{
  RowBox[{"(*", 
   RowBox[{
   "Define", " ", "the", " ", "radius", " ", "of", " ", "the", " ", 
    "sphere"}], "*)"}], 
  RowBox[{
   RowBox[{
    RowBox[{"r", "=", "1"}], ";"}], "\[IndentingNewLine]", "\n", 
   RowBox[{"(*", 
    RowBox[{
    "Plot", " ", "the", " ", "sphere", " ", "in", " ", "the", " ", "first", 
     " ", "octant"}], "*)"}], "\[IndentingNewLine]", 
   RowBox[{
    RowBox[{"octant", "=", 
     RowBox[{"RegionPlot3D", "[", 
      RowBox[{
       RowBox[{
        RowBox[{
         RowBox[{
          RowBox[{"x", "^", "2"}], "+", 
          RowBox[{"y", "^", "2"}], "+", 
          RowBox[{"z", "^", "2"}]}], "<=", 
